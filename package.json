{
  "name": "Ts-React-Starter",
  "version": "1.0.0",
  "description": "A Typescript React Starter with Vue-cli",
  "author": "haokur <haokur@qq.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "local": "node build/dev-local.js",
    "local:re": "node build/dev-local.js --rebuild",
    "doc": "docsify serve ./docs",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "axios": "^0.17.1",
    "prop-types": "^15.6.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/axios": "^0.14.0",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.27",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/redux": "^3.6.0",
    "ajv": "6.5.0",
    "autoprefixer": "7.1.2",
    "awesome-typescript-loader": "3.5.0",
    "babel-core": "6.22.1",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-runtime": "6.22.0",
    "babel-preset-env": "1.3.2",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.22.0",
    "chalk": "2.0.1",
    "connect-history-api-fallback": "1.3.0",
    "conventional-changelog": "^3.1.8",
    "conventional-changelog-cli": "^2.0.21",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.0",
    "cssnano": "3.10.0",
    "cz-conventional-changelog": "^2.1.0",
    "cz-customizable": "^6.1.0",
    "ecstatic": "3.2.0",
    "eslint-friendly-formatter": "3.0.0",
    "eventsource-polyfill": "0.9.6",
    "express": "4.14.1",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "0.11.1",
    "friendly-errors-webpack-plugin": "1.1.3",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.3",
    "http-server": "0.11.1",
    "husky": "^2.3.0",
    "lazy-load-component": "^1.1.5",
    "loadash": "1.0.0",
    "node-notifier": "5.1.2",
    "node-sass": "4.9.0",
    "opn": "5.1.0",
    "optimist": "0.6.1",
    "optimize-css-assets-webpack-plugin": "2.0.0",
    "ora": "1.2.0",
    "postcss": "6.0.14",
    "postcss-browser-reporter": "0.5.0",
    "postcss-cssnext": "3.0.2",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.9",
    "postcss-reporter": "5.0.0",
    "postcss-url": "7.3.0",
    "react-hot-loader": "3.1.3",
    "rimraf": "2.6.0",
    "sass-loader": "6.0.6",
    "semver": "5.3.0",
    "shelljs": "0.7.6",
    "style-loader": "0.19.1",
    "typescript": "2.9.2",
    "url-loader": "0.5.8",
    "vue-loader": "13.0.4",
    "vue-style-loader": "3.0.1",
    "vue-template-compiler": "2.5.13",
    "webpack": "2.6.1",
    "webpack-bundle-analyzer": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-dev-server": "2.11.2",
    "webpack-hot-middleware": "2.18.0",
    "webpack-merge": "4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
